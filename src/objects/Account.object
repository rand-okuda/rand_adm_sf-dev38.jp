    <fields>
        <fullName>EmployeeNumberCode__c</fullName>
        <externalId>false</externalId>
        <formula>IF (
 AND(
 4999  &gt;= EmployeeNumber__c, 
 EmployeeNumber__c  &gt;= 1000
 ),
 &apos;B&apos;,
IF (
 AND(
 999  &gt;= EmployeeNumber__c, 
 EmployeeNumber__c  &gt;= 300
 ),
 &apos;M&apos;,
IF (
 AND(
 299  &gt;= EmployeeNumber__c, 
 EmployeeNumber__c  &gt;= 0
 ),
 &apos;S&apos;,
 &apos;O&apos;
) 
) 
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
