    <fields>
        <fullName>BillingStatus__c</fullName>
        <externalId>false</externalId>
        <formula>IF (
 NOT(Opportunity__r.Monitoring__c) ,
 &apos;請求書生成待ち&apos;,
IF (
 AND(
 Opportunity__r.Monitoring__c ,
 NOT( PaymentConfirmed__c ) ,
 JobOfferRefusal__c ,
 Product2__r.RefundObligation__c 
 ) ,
 &apos;辞退により入金不要&apos; ,
IF (
 AND(
 Opportunity__r.Monitoring__c ,
 PaymentConfirmed__c ,
 JobOfferRefusal__c ,
 Product2__r.RefundObligation__c 
 ) ,
 &apos;返金処理&apos; ,
IF (
 AND(
 Opportunity__r.Monitoring__c ,
 Opportunity__r.PaymentDueDateFixed__c  &gt;  TODAY() ,
 NOT( JobOfferRefusal__c ) ,
 NOT( PaymentConfirmed__c )
 ) ,
 &apos;入金待ち&apos; ,
IF (
 AND(
 Opportunity__r.Monitoring__c ,
 Opportunity__r.PaymentDueDateFixed__c  &lt;  TODAY() ,
 NOT( JobOfferRefusal__c ) ,
 NOT( PaymentConfirmed__c ) 
 ) ,
 &apos;入金督促&apos; ,
IF (
 AND(
 Opportunity__r.Monitoring__c ,
 PaymentConfirmed__c
 ) ,
 &apos;回収完了&apos;,
 &apos;&apos;
) 
) 
) 
) 
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>請求ステータス</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
